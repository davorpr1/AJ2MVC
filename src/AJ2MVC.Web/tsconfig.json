{
  "compilerOptions": {
    "removeComments": true,
    "preserveConstEnums": true,
    "declaration": true,
    "sourceMap": false,
    "target": "es5",
    "module": "system",
    "moduleResolution": "node",
    
    "outDir": "./dist/",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "dist"
  ]
}
